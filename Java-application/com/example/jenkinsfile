pipeline {
    agent any
    stages {
        stage('Code Checkout') {
            steps {
                // Checkout the code from the version control (e.g., Git)
                git branch: 'main', url: 'https://github.com/your-repo/java-app.git'
            }
        }
        
        stage('Build with Maven') {
            steps {
                // Use Maven to clean, compile, and package the application
                sh 'mvn clean package'
            }
        }
    }
    
